<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation_tab"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="cs.hku.wallpaper.ui.online.OnlineFragment"
        android:label="@string/title_home"
        tools:layout="@layout/fragment_old_home" />

    <fragment
        android:id="@+id/navigation_dashboard"
        android:name="cs.hku.wallpaper.ui.local.LocalFragment"
        android:label="@string/title_dashboard"
        tools:layout="@layout/fragment_dashboard" />

    <fragment
        android:id="@+id/navigation_notifications"
        android:name="cs.hku.wallpaper.ui.user.UserFragment"
        android:label="@string/title_notifications"
        tools:layout="@layout/fragment_notifications" />
</navigation>
